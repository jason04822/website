<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Tool Sets</title>
    <style>
        table {
            width: 100%;
            border-collapse: collapse;
            margin-top: 20px;
        }
        th, td {
            border: 1px solid #ddd;
            padding: 8px;
            text-align: left;
        }
        th {
            background-color: #f2f2f2;
        }
        tr:nth-child(even) {
            background-color: #f9f9f9;
        }
    </style>
</head>
<body>
    <h1>Tool Sets List</h1>
    <a href="/">Back to Home</a>
    
    <% if (toolSets.length > 0) { %>
        <table>
            <thead>
                <tr>
                    <th>ID</th>
                    <th>Name</th>
                    <th>Category</th>
                    <th>Subtype</th>
                    <th>Created At</th>
                </tr>
            </thead>
            <tbody>
                <% toolSets.forEach(tool => { %>
                    <tr>
                        <td><%= tool.id %></td>
                        <td><%= tool.name %></td>
                        <td><%= tool.category %></td>
                        <td><%= tool.subtype %></td>
                        <td><%= tool.created_at %></td>
                    </tr>
                <% }); %>
            </tbody>
        </table>
    <% } else { %>
        <p>No tool sets found.</p>
    <% } %>
</body>
</html>